{
	"name": "uniam-api",
	"type": "module",
	"scripts": {
		"dev": "wrangler dev",
		"deploy": "wrangler deploy --minify",
		"cf-typegen": "wrangler types --env-interface CloudflareBindings",
		"test": "vitest run",
		"test:watch": "vitest",
		"lint": "biome lint .",
		"format": "biome format --write .",
		"check": "biome check --write .",
		"prepare": "lefthook install",
		"r2:create-bucket": "wrangler r2 bucket create uniam-prompts",
		"r2:upload-prompts": "wrangler r2 object put uniam-prompts/prompts/cat-diagnosis.json --file=./prompts/cat-diagnosis.json",
		"r2:upload-prompts:remote": "wrangler r2 object put uniam-prompts/prompts/cat-diagnosis.json --file=./prompts/cat-diagnosis.json --remote",
		"r2:get-prompt": "wrangler r2 object get uniam-prompts/prompts/cat-diagnosis.json",
		"r2:get-prompt:remote": "wrangler r2 object get uniam-prompts/prompts/cat-diagnosis.json --remote",
		"r2:list-buckets": "wrangler r2 bucket list"
	},
	"dependencies": {
		"@google/genai": "^1.33.0",
		"hono": "^4.10.8"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.8",
		"@cloudflare/vitest-pool-workers": "^0.10.14",
		"@cloudflare/workers-types": "^4.20251210.0",
		"lefthook": "^2.0.9",
		"vitest": "^2.1.9",
		"wrangler": "^4.4.0"
	}
}
